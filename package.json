{
  "name": "ui-components-build-template",
  "version": "0.2.0",
  "main": "./lib/index.js",
  "private": true,
  "engines": {
    "node": ">4.0.0"
  },
  "author": {
    "name": "Austin Knight",
    "email": "austinknight@austinknight.net",
    "url": "http://austinknight.net"
  },
  "files": [
    "lib"
  ],
  "keywords": [],
  "dependencies": {
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "styled-components": "^2.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.1.2",
    "@storybook/react": "^3.1.3",
    "autoprefixer": "7.1.0",
    "babel-cli": "^6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-react-app": "^3.0.0",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "chokidar": "^1.7.0",
    "cpx": "^1.5.0",
    "cross-env": "^5.0.1",
    "css-loader": "0.28.1",
    "dotenv": "4.0.0",
    "enzyme": "^2.8.2",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "fs-extra": "3.0.1",
    "gh-pages": "^1.0.0",
    "highlight.js": "^9.12.0",
    "html-webpack-plugin": "2.28.0",
    "jest": "20.0.3",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "npm-run-all": "^4.0.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.5",
    "promise": "7.1.1",
    "prop-types": "^15.5.10",
    "react-addons-test-utils": "^15.6.0",
    "react-dev-utils": "^3.0.0",
    "react-docgen": "^2.15.0",
    "react-error-overlay": "^1.0.7",
    "react-input-range": "^1.1.4",
    "react-storybook-addon-chapters": "^1.1.0",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "style-loader": "0.17.0",
    "sw-precache-webpack-plugin": "0.9.1",
    "url-loader": "0.5.8",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "homepage": "http://austinknight.net/ui-components/",
  "scripts": {
    "prestart": "npm run gen:docs",
    "start": "npm-run-all --parallel start:docs start:storybook gen:docs-watch",
    "start:docs": "node scripts/start.js",
    "start:storybook": "start-storybook -p 3001 -c .storybook",
    "gen:docs": "node scripts/generateComponentData.js",
    "gen:docs-watch": "npm run gen:docs -- --watch",
    "build:docs": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "prebuild:lib": "rimraf lib",
    "build:lib": "npm-run-all --parallel build:commonjs build:copy-files",
    "build:copy-files": "node scripts/copyBuildFiles.js",
    "predeploy:docs": "npm-run-all --parallel build:docs build:storybook",
    "deploy:docs": "gh-pages -d build",
    "build:commonjs": "cross-env NODE_ENV=production babel ./src/components --out-dir ./lib --ignore spec.js --ignore stories.js",
    "build:storybook": "build-storybook -c .storybook -o ./build/storybook",
    "prepublish": "npm run build:lib"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
